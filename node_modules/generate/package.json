  "lintDeps": {
    "ignore": [
      "code",
      "coverage",
      "docs",
      "examples",
      "lib/tree.js",
      "support"
    ]
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "app",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "update",
      "description": "Generate shares a common architecture and plugin ecosystem with the following libraries:",
      "list": [
        "assemble",
        "base",
        "update",
        "verb"
      ]
    },
    "reflinks": [
      "assemble",
      "assemble-core",
      "bach",
      "base",
      "common-config",
      "consolidate",
