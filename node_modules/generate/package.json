    "base-pipeline": "^0.3.2",
    "base-questions": "^0.7.4",
    "base-runner": "^0.8.2",
    "base-runtimes": "^0.2.0",
    "base-store": "^0.4.4",
    "bdd-stdin": "^0.2.0",
    "common-config": "^0.1.0",
    "data-store": "^0.16.1",
    "debug": "^2.2.0",
    "define-property": "^0.2.5",
    "export-files": "^2.1.1",
    "extend-shallow": "^2.0.1",
    "fs-exists-sync": "^0.1.0",
