    "gulp-mocha": "^3.0.0",
    "gulp-unused": "^0.1.2",
    "is-absolute": "^0.2.5",
    "mocha": "^3.0.2",
    "npm-install-global": "^0.1.2",
    "resolve": "^1.1.7",
    "should": "^10.0.0"
