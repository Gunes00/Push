    "get-value": "^2.0.6",
    "global-modules": "^0.2.3",
    "helper-ask": "^0.1.4",
    "isobject": "^2.1.0",
    "lazy-cache": "^2.0.1",
    "log-utils": "^0.2.1",
    "macro-store": "^0.1.0",
    "resolve-file": "^0.2.0",
    "set-blocking": "^2.0.0",
    "strip-color": "^0.1.0",
    "text-table": "^0.2.0",
    "through2": "^2.0.1",
    "yargs-parser": "^3.1.0"
  },
  "devDependencies": {
    "base-config-process": "^0.1.9",
    "base-test-runner": "^0.2.0",
    "base-test-suite": "^0.2.4",
    "cross-spawn": "^4.0.0",
    "generate-foo": "^0.1.5",
    "generator-util": "^0.2.9",
    "glob-keys": "^0.1.1",
    "gulp": "^3.9.1",
